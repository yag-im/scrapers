{
	"name": "yag-scrapers",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.black-formatter"
			]
		}
	},
	"postCreateCommand": ".devcontainer/post-create.sh",
	"mounts": [
		"source=${env:HOME}${env:USERPROFILE}/.ssh,target=/home/vscode/.ssh,type=bind"
	]
}
